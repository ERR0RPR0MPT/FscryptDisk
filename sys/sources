TARGETNAME=fscryptdisk
TARGETPATH=.
TARGETTYPE=DRIVER

MSC_WARNING_LEVEL=/W4 /WX /wd4201 /wd4204 /wd4221 /wd4482
!IF "$(NTDEBUG)" != "ntsd"
MSC_OPTIMIZATION=/Ox /GF
!ENDIF
C_DEFINES=$(C_DEFINES) /DINCLUDE_GPL_ORIGIN

!IF "$(_BUILDARCH)" == "x86"
LINKER_FLAGS=llmath.lib
BUFFER_OVERFLOW_CHECKS=0
!ENDIF

SOURCES=fscryptdisk.cpp \
  commonio.cpp \
  createdev.cpp \
  devthrd.cpp \
  floppy.cpp \
  iodisp.cpp \
  lowerdev.cpp \
  proxy.cpp \
  fscryptdisk.rc

